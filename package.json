{
  "name": "broccoli-dependency-funnel",
  "version": "1.0.0",
  "description": "Funnels a set of files included (or excluded) from a JS dependency graph",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && broccoli build dist",
    "build:serve": "broccoli serve",
    "test": "npm run build && mocha dist/tests",
    "test:lint": "npm run build && mocha dist/tests/lint.js",
    "test:debug": "npm run build && mocha debug dist/tests",
    "prepublish": "npm run build"
  },
  "keywords": [
    "broccoli",
    "plugin"
  ],
  "contributors": [
    "Nathan Hammond",
    "Robert Jackson",
    "Trent Willis"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-engines/broccoli-dependency-funnel.git"
  },
  "bugs": {
    "url": "https://github.com/ember-engines/broccoli-dependency-funnel/issues"
  },
  "homepage": "https://github.com/ember-engines/broccoli-dependency-funnel#readme",
  "files": [
    "dist/",
    "!dist/tests"
  ],
  "devDependencies": {
    "broccoli": "^0.16.9",
    "broccoli-babel-transpiler": "^5.6.0",
    "broccoli-cli": "^1.0.0",
    "broccoli-lint-eslint": "^2.7.0",
    "broccoli-merge-trees": "^1.1.4",
    "broccoli-stew": "^1.3.2",
    "chai": "^3.5.0",
    "chai-files": "^1.4.0",
    "es6-promise": "^3.2.1",
    "fixturify": "^0.3.0",
    "fs-extra": "^0.30.0",
    "mocha": "^2.5.3",
    "mocha-eslint": "^2.1.1",
    "regenerator-runtime": "^0.9.5",
    "walk-sync": "^0.3.1"
  },
  "dependencies": {
    "amd-name-resolver": "^0.0.6",
    "broccoli-plugin": "^1.2.1",
    "exists-sync": "^0.0.4",
    "fs-tree-diff": "^0.5.2",
    "heimdalljs": "^0.2.1",
    "heimdalljs-logger": "^0.1.7",
    "mkdirp": "^0.5.1",
    "path-posix": "^1.0.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.4",
    "symlink-or-copy": "^1.1.8"
  }
}
